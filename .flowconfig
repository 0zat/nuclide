[include]

[ignore]
# second-level dependencies
.*/node_modules/[^/]+/node_modules/.*

# non-flowCheck
.*/pkg/fb/biggrep-provider/.*
.*/pkg/fb/phabricator-comments-client/.*
.*/pkg/fb/test-runner-t-client/.*
.*/pkg/nuclide/buck/files/.*
.*/pkg/nuclide/debugger-hhvm-proxy/spec/.*
.*/pkg/nuclide/debugger/atom/spec/.*
.*/pkg/nuclide/debugger/hhvm/.*
.*/pkg/nuclide/debugger/node/.*
.*/pkg/nuclide/file-tree/.*
.*/pkg/nuclide/format-js/.*
.*/pkg/nuclide/fuzzy-filename-provider/.*
.*/pkg/nuclide/hack-symbol-provider/.*
.*/pkg/nuclide/hack/.*
.*/pkg/nuclide/quick-open/.*
.*/pkg/nuclide/remote-connection/spec/.*
.*/pkg/nuclide/remote-projects/spec/.*
.*/pkg/nuclide/server/spec/.*
.*/pkg/nuclide/server/scripts/.*
.*/pkg/nuclide/ssh-dialog/.*
.*/pkg/nuclide/toolbar/.*

[libs]
./pkg/nuclide/external-interfaces/1.0/
./pkg/nuclide/debugger/interfaces/WebInspector.js

[options]
esproposal.decorators=ignore
suppress_comment=.*\\$FlowFixMe.*
suppress_comment=.*\\$FlowIssue.*
suppress_comment=.*\\$FlowIgnore.*
unsafe.enable_getters_and_setters=true

[version]
0.19.0
