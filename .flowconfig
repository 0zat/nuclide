[include]

[ignore]
# second-level dependencies
.*/node_modules/[^/]+/node_modules/.*
# becomes first-level dependency with npm3
.*/node_modules/fbjs/flow/.*
.*/node_modules/babel-types/.*
# ignore module src to prefer declaration
.*/node_modules/rx/.*

[libs]
./pkg/nuclide/external-interfaces/1.0/
./pkg/nuclide/debugger/interfaces/WebInspector.js

[options]
esproposal.class_static_fields=enable
esproposal.decorators=ignore
suppress_comment=.*\\$FlowFixMe.*
suppress_comment=.*\\$FlowIssue.*
suppress_comment=.*\\$FlowIgnore.*
unsafe.enable_getters_and_setters=true

[version]
0.20.1
