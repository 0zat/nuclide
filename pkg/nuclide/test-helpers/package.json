{
  "name": "nuclide-test-helpers",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Provides utility functions for creating package test suites",
  "nuclide": {
    "flowCheck": true,
    "packageType": "Node",
    "testRunner": "npm"
  },
  "dependencies": {
    "chalk": "1.0.0",
    "diff": "1.4.0"
  },
  "devDependencies": {
    "nuclide-external-interfaces": "0.0.0",
    "nuclide-jasmine": "0.0.0"
  },
  "scripts": {
    "test": "node --harmony node_modules/.bin/jasmine-node-transpiled spec"
  }
}
