{
  "name": "nuclide-home",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Provides a welcome message and other links for first time users.",
  "nuclide": {
    "config": {
      "showHome": {
        "type": "boolean",
        "default": true,
        "description": "Show the home pane (by default shown on first startup)."
      }
    },
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "nuclide-home.homeFragments": {
      "versions": {
        "0.0.0": "setHomeFragments"
      }
    }
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "dependencies": {
    "nuclide-commons": "0.0.0",
    "nuclide-feature-config": "0.0.0",
    "react-for-atom": "0.14.3"
  },
  "devDependencies": {
    "nuclide-home-interfaces": "0.0.0"
  },
  "_atomModuleCache": {
    "extensions": {
      ".node": []
    }
  }
}
