{
  "name": "nuclide-home",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Provides a welcome message and other links for first time users.",
  "nuclide": {
    "config": {
      "showHome": {
        "type": "boolean",
        "default": true,
        "description": "Show the home pane (by default shown on first startup)."
      }
    },
    "deserializers": {
      "HomePaneItem": "./lib/deserializePaneItem"
    },
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "nuclide-home.homeFragments": {
      "versions": {
        "0.0.0": "setHomeFragments"
      }
    }
  },
  "engines": {
    "atom": ">=1.0.0"
  },
  "_atomModuleCache": {
    "extensions": {
      ".node": []
    }
  }
}
