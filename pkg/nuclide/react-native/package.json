{
  "name": "nuclide-react-native",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "React Native development tools",
  "nuclide": {
    "config": {
      "pathToNode": {
        "title": "Path to Node Executable",
        "type": "string",
        "default": "node",
        "description": "Absolute path to the node executable on your system. This will be used for executing JavaScript when debugging."
      }
    },
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "activationCommands": {
    "atom-workspace": [
      "nuclide-react-native:start-debugging",
      "nuclide-react-native:stop-debugging",
      "nuclide-react-native:start-packager",
      "nuclide-react-native:stop-packager",
      "nuclide-react-native:restart-packager",
      "nuclide-react-native:reload-app"
    ]
  }
}
