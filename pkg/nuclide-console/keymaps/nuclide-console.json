{
  ".platform-darwin atom-workspace": {
    "cmd-alt-j": "nuclide-console:toggle"
  },
  ".platform-win32 atom-workspace, .platform-linux atom-workspace": {
    "ctrl-shift-j": "nuclide-console:toggle"
  },
  ".platform-darwin atom-workspace": {
    "ctrl-l": "nuclide-console:clear"
  },
  ".platform-win32 atom-workspace, .platform-linux atom-workspace": {
    "ctrl-l": "nuclide-console:clear"
  }
}
